doctype html
html
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    nav.navbar.navbar-expand-lg.navbar-dark.bg-dark
      a.navbar-brand(href="/") Brand
      button.navbar-toggler(type="button", data-toggle="collapse", data-target="#navbarMain")
        span.navbar-toggler-icon
      .collapse.navbar-collapse#navbarMain
        ul.navbar-nav
          li.nav-item
            a.nav-link(href="/pages/link") Link
        ul.navbar-nav.ml-auto
          li.nav-item
            a.nav-link(href="/pages/login") Log In
          li.nav-item
            a.nav-link(href="/pages/register") Register
    .container
      block content
      footer
        block notes
    script(src="/scripts/jquery/dist/jquery.slim.js")
    script(src="/scripts/popper.js/dist/umd/popper.js")
    script(src="/scripts/bootstrap/dist/js/bootstrap.js")
